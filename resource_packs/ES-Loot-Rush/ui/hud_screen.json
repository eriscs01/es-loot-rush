{
  "namespace": "hud",
  "root_panel": {
    "modifications": [
      {
        "array_name": "controls",
        "operation": "insert_front",
        "value": {
          "eslr_hud_elements@eslr_hud_elements": {}
        }
      }
    ]
  },
  "hud_title_text/title_frame/title": {
    "modifications": [
      {
        "array_name": "bindings",
        "operation": "insert_back",
        "value": [
          {
            "binding_type": "view",
            "source_property_name": "((#text - 'update') = #text)",
            "target_property_name": "#visible"
          }
        ]
      }
    ]
  },
  "eslr_hud_elements": {
    "type": "panel",
    "controls": [
      { "round_grid@round_indicator.round_grid": {} },
      { "timer_grid@timer_indicator.timer_grid": {} },
      { "score_azure_grid@score_azure_indicator.score_azure_grid": {} },
      { "score_crimson_grid@score_crimson_indicator.score_crimson_grid": {} },

      { "challenge_header_label@challenge_header_indicator.challenge_header_label": {} },
      { "challenge0_label@challenge0_indicator.challenge0_label": {} },
      { "challenge1_label@challenge1_indicator.challenge1_label": {} },
      { "challenge2_label@challenge2_indicator.challenge2_label": {} },
      { "challenge3_label@challenge3_indicator.challenge3_label": {} },
      { "challenge4_label@challenge4_indicator.challenge4_label": {} },
      { "challenge5_label@challenge5_indicator.challenge5_label": {} },
      { "challenge6_label@challenge6_indicator.challenge6_label": {} },
      { "challenge7_label@challenge7_indicator.challenge7_label": {} },
      { "challenge8_label@challenge8_indicator.challenge8_label": {} },
      { "challenge9_label@challenge9_indicator.challenge9_label": {} }
    ]
  }
}
